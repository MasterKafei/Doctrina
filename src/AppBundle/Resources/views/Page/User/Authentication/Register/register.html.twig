<div id="Register" class="tabcontent">
    <img class="mx-auto d-block" src="{{ asset('assets/svg/logo.svg') }}">
    {{ form_start(form, {'action': path('app_user_authentication_signing_register')}) }}
    <div class="sign-form">
        <div class="form-group">
            {{ form_label(form.idBooster) }}
            {{ form_widget(form.idBooster, {'attr': {'class': 'form-control', 'placeholder': 'user.authentication.register.id_booster'|trans({}, 'placeholder')}}) }}
        </div>
        <div class="form-group">
            {{ form_label(form.plainPassword.first) }}
            {{ form_widget(form.plainPassword.first, {'attr': {'class': 'form-control', 'placeholder': 'user.authentication.register.password'|trans({}, 'placeholder')}}) }}
        </div>
        <div class="form-group">
            {{ form_label(form.plainPassword.second) }}
            {{ form_widget(form.plainPassword.second, {'attr': {'class': 'form-control', 'placeholder': 'user.authentication.register.confirm_password'|trans({}, 'placeholder')}}) }}
        </div>
    </div>
    {{ form_widget(form.submit, {'attr': {'class': 'btn custom-full-btn'}}) }}
    {{ form_end(form) }}
</div>